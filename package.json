{
  "name": "@snapboard/ui",
  "version": "0.16.0",
  "description": "A library for interacting with Snapboard",
  "main": "./dist/index.js",
  "module": "./dist/es/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/snapboard/snapboard-core.git"
  },
  "scripts": {
    "start": "yarn storybook",
    "build": "yarn clean && yarn build:library",
    "build:library": "./scripts/build-library.js",
    "build:package": "./scripts/build-package.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:debug": "node --inspect --inspect-brk node_modules/.bin/jest",
    "test:watch": "yarn test -- --watch",
    "version": "yarn build",
    "release": "npm run build && npm run build:package && ./scripts/release.js",
    "pack-ls": "cd dist && tar -tvf $(npm pack) && rm *.tgz",
    "coveralls": "NODE_ENV=spec nyc yarn test && nyc report --reporter=text-lcov | coveralls",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "^10.0.21",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-react-app": "^9.0.2",
    "eslint": "^6.5.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.10.2",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^14.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "chart.js": "^2.9.4",
    "color": "^3.1.2",
    "funnel-graph-js": "^1.3.9",
    "lodash": "^4.17.15",
    "mineral-ui": "^0.58.0",
    "mineral-ui-icons": "^0.5.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react-chartjs-2": "^2.8.0",
    "react-measure": "^2.3.0",
    "react-resize-detector": "^4.2.1",
    "react-table": "^6.10.3",
    "recompose": "^0.30.0"
  }
}
